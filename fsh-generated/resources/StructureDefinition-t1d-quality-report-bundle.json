{
  "resourceType": "StructureDefinition",
  "id": "t1d-quality-report-bundle",
  "url": "http://t1dexchange.org/fhir/StructureDefinition/t1d-quality-report-bundle",
  "name": "T1DQualityReportBundle",
  "title": "T1D Quality Report Bundle",
  "status": "draft",
  "description": "Bundle for T1D Exchange Quality Improvement reporting, containing aggregated data for population health analysis and benchmarking.",
  "fhirVersion": "4.0.1",
  "kind": "resource",
  "abstract": false,
  "type": "Bundle",
  "baseDefinition": "http://hl7.org/fhir/StructureDefinition/Bundle",
  "derivation": "constraint",
  "differential": {
    "element": [
      {
        "id": "Bundle.type",
        "path": "Bundle.type",
        "fixedCode": "collection"
      },
      {
        "id": "Bundle.timestamp",
        "path": "Bundle.timestamp",
        "min": 1,
        "mustSupport": true
      },
      {
        "id": "Bundle.entry",
        "path": "Bundle.entry",
        "slicing": {
          "discriminator": [
            {
              "type": "type",
              "path": "resource"
            }
          ],
          "rules": "open"
        },
        "min": 1,
        "mustSupport": true
      },
      {
        "id": "Bundle.entry:organization",
        "path": "Bundle.entry",
        "sliceName": "organization",
        "min": 1,
        "max": "1",
        "mustSupport": true
      },
      {
        "id": "Bundle.entry:organization.resource",
        "path": "Bundle.entry.resource",
        "short": "Reporting organization",
        "type": [
          {
            "code": "Organization",
            "profile": [
              "http://t1dexchange.org/fhir/StructureDefinition/t1d-organization"
            ]
          }
        ]
      },
      {
        "id": "Bundle.entry:measureReport",
        "path": "Bundle.entry",
        "sliceName": "measureReport",
        "min": 0,
        "max": "*",
        "mustSupport": true
      },
      {
        "id": "Bundle.entry:measureReport.resource",
        "path": "Bundle.entry.resource",
        "short": "Quality measure reports",
        "type": [
          {
            "code": "MeasureReport"
          }
        ]
      },
      {
        "id": "Bundle.entry:observation",
        "path": "Bundle.entry",
        "sliceName": "observation",
        "min": 0,
        "max": "*",
        "mustSupport": true
